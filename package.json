{
  "name": "emoji-lib",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/OlgaMyazina/emoji.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "tsc && webpack",
    "build-storybook": "build-storybook -s public --max_old_space_size=4096",
    "build-storybook-docs": "build-storybook --docs",
    "check-types": "tsc --noEmit",
    "copy-files": "cp package.json dist/package.json && cp LICENSE dist/LICENSE && cp readme.md dist/readme.md",
    "eject": "react-scripts eject",
    "eslint": "eslint --ignore-path .gitignore .",
    "lint": "npm run prettier -- --write && npm run eslint -- --fix && npm run stylelint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(html|js|json|jsx|sass|yml)\"",
    "prepare": "husky install",
    "release": "npm run build && npm run copy-files && npm publish dist",
    "start": "react-scripts --max_old_space_size=4096 start",
    "storybook": "start-storybook -p 6006 -s public",
    "stylelint": "stylelint --ignore-path .gitignore \"**/*.+(css|scss)\"",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "classnames": "^2.3.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.54.3",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0"
  }
}
